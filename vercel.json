{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "env": {
      "MONGO_URI": "@MONGO_URI",
      "JWT_SECRET": "@JWT_SECRET",
      "STRIPE_SECRET_KEY": "@STRIPE_SECRET_KEY"
    }
  }
  